<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>used to enable admin control of person note styles i.e. colour based on text of person note</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>CSS__c</fullName>
        <description>will be applied to the td tag that contains the person note text</description>
        <externalId>false</externalId>
        <inlineHelpText>will be applied to the td tag that contains the person note text e.g. &apos;background:red;colour:white;&apos;. No need for quotation marks.</inlineHelpText>
        <label>CSS</label>
        <length>200</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Person Note Style</label>
    <visibility>Public</visibility>
</CustomObject>
