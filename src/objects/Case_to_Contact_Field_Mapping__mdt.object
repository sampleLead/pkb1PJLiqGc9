<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>used to define a case field (API name) mapping to a Contact field (API name) of same type.</description>
    <fields>
        <fullName>Case_API_name__c</fullName>
        <description>the case field to be mapped - API name only</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <label>Case Field API name</label>
        <length>200</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Contact_Field_API_name__c</fullName>
        <description>contact api field name to map to</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <label>Contact Field API name</label>
        <length>200</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Use_in_webform_mapping__c</fullName>
        <defaultValue>false</defaultValue>
        <description>if checked, then this mapping will be used to map values from a webform onto a matched contact</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <label>Use in webform mapping</label>
        <type>Checkbox</type>
    </fields>
    <label>Case to Contact Field Mapping</label>
    <pluralLabel>Case to Contact Field Mappings</pluralLabel>
    <visibility>Public</visibility>
</CustomObject>
